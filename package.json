{
  "name": "sanremo",
  "version": "0.2.0",
  "private": true,
  "type": "module",
  "scripts": {
    "start": "node --es-module-specifier-resolution=node ./server-build/src",
    "serverLocal": "DATABASE_URL=postgres://scdf@localhost PORT=5000 npm start",
    "build": "npm run buildClient && npm run buildServer",
    "buildClient": "react-scripts build",
    "buildServer": "rm -r ./server-build; tsc --build tsconfig.server.json",
    "dev": "react-scripts start",
    "test": "react-scripts test",
    "analyze": "source-map-explorer 'build/static/js/*.js' --html result.html",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "connect-pg-simple": "^6.2.1",
    "express": "^4.17.1",
    "express-session": "^1.17.1",
    "pg": "^8.6.0"
  },
  "devDependencies": {
    "@material-ui/core": "^4.11.3",
    "@material-ui/icons": "^4.11.2",
    "@testing-library/jest-dom": "^5.11.9",
    "@testing-library/react": "^11.2.5",
    "@testing-library/user-event": "^12.8.3",
    "@types/axios": "^0.14.0",
    "@types/bcryptjs": "^2.4.2",
    "@types/connect-pg-simple": "^4.2.4",
    "@types/express": "^4.17.13",
    "@types/express-session": "^1.17.4",
    "@types/jest": "^26.0.24",
    "@types/node": "^14",
    "@types/pouchdb": "^6.4.0",
    "@types/react": "^17.0.14",
    "@types/react-dom": "^17.0.9",
    "axios": "^0.21.1",
    "date-fns": "^2.17.0",
    "debug": "^4.3.1",
    "dotenv": "^8.2.0",
    "history": "^5.0.0",
    "pouchdb-adapter-idb": "^7.2.2",
    "pouchdb-browser": "^7.2.2",
    "pouchdb-find": "^7.2.2",
    "prettier": "^2.3.2",
